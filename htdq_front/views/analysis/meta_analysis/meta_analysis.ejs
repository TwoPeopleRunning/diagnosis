<!DOCTYPE html>
<html>

<head>
    <title>综合分析</title>
    <meta charset="utf-8">
    <link href="/content/shared/styles/examples-offline.css" rel="stylesheet">
    <link href="/styles/kendo.common.min.css" rel="stylesheet">
    <link href="/styles/kendo.rtl.min.css" rel="stylesheet">
    <link href="/styles/kendo.default.min.css" rel="stylesheet">
    <link href="/styles/kendo.default.mobile.min.css" rel="stylesheet">
    <script src="/js/jquery.min.js"></script>
    <script src="/js/jszip.min.js"></script>
    <script src="/js/kendo.all.min.js"></script>
    <script src="/content/shared/js/console.js"></script>
</head>

<div class="mainchart">
    <button class='export-image k-button'>导出Image</button>
    <div id="chart_01" style="height:230px;"></div>
    <div id="chart_02" style="height:230px"></div>
    <div id="chart_03" style="height:230px"></div>
</div>
<script src="/analysis/js/statement.js"></script>
<script src="/analysis/js/perf_analysis.js"></script>
<script>
    $(".export-image").click(function () {
        $("#chart_01").getKendoChart().exportImage({ paperSize: "auto", margin: { left: "1cm", top: "1cm", right: "1cm", bottom: "1cm" } }).done(function (data) {
            kendo.saveAs({
                dataURI: data,
                fileName: "设备运行效率.png",
            });
        });
        $("#chart_02").getKendoChart().exportImage({ paperSize: "auto", margin: { left: "1cm", top: "1cm", right: "1cm", bottom: "1cm" } }).done(function (data) {
            kendo.saveAs({
                dataURI: data,
                fileName: "设备总利用率.png",
            });
        });
        $("#chart_03").getKendoChart().exportImage({ paperSize: "auto", margin: { left: "1cm", top: "1cm", right: "1cm", bottom: "1cm" } }).done(function (data) {
            kendo.saveAs({
                dataURI: data,
                fileName: "设备总开机率.png",
            });
        });
    });

</script>


<style>
    .mainchart {
        height: "100%";
    }

    /* #chart_01,
    #chart_02,
    #chart_03 {
        height: 200px;
    } */
</style>

</html>