<!DOCTYPE html>
<html>

<head>
    <title>设备总利用率</title>
    <meta charset="utf-8">
    <link href="/content/shared/styles/examples-offline.css" rel="stylesheet">
    <link href="/styles/kendo.common.min.css" rel="stylesheet">
    <link href="/styles/kendo.rtl.min.css" rel="stylesheet">
    <link href="/styles/kendo.default.min.css" rel="stylesheet">
    <link href="/styles/kendo.default.mobile.min.css" rel="stylesheet">
    <script src="/js/jquery.min.js"></script>
    <script src="/js/jszip.min.js"></script>
    <script src="/js/kendo.all.min.js"></script>
    <script src="/content/shared/js/console.js"></script>
</head>

<div class="demo-section k-content wide">
    <button class='k-button' id="image2">导出Image</button>
    <div id="chartTrend2"></div>
</div>
<script src="/analysis/js/statement.js"></script>
<script src="/analysis/js/Trend_analysis.js"></script>
<script>
    $("#image2").click(function () {
        $("#chartTrend2").getKendoChart().exportImage({ paperSize: "auto", margin: { left: "1cm", top: "1cm", right: "1cm", bottom: "1cm" } }).done(function (data) {
            kendo.saveAs({
                dataURI: data,
                fileName: "设备总利用率走势图.png",
            });
        });
    });
</script>
