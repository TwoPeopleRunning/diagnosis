<!--     设备统计状态 ####################################################################### -->
<div id="example" style="margin:0px">
    <table id="grid4">
    </table>
    <table class="k-grid-content k-widget k-grid" style="margin-top:-15px">
        <colgroup>
            <col style="width:20%" />
            <col style="width:10%" /><col style="width:10%" /><col style="width:10%" />
            <col style="width:10%" /><col style="width:10%" /><col style="width:10%" />
            <col style="width:10%" /><col style="width:10%" />
        </colgroup>  
    <tr class="k-alt">
        <td>合计</td>
        <td class="TotalRun"></td>
        <td class="TotalOnline"></td>
        <td class="TotalAlarm"></td>
        <td class="TotalOffline"></td >
        <td class="AvaUse"></td>
        <td class="AvaOnline"></td>
        <td class="AvaAlarm"></td>
